}


QComboBox:on
{
    background-color: transparent;
	color: #fff;

}


QComboBox QAbstractItemView
{
    background-color: transparent;
    color: #fff;
    selection-background-color: #fea732;
	selection-color: #000;
    outline: 0;

}
